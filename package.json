{
	"name": "scraping-notification",
	"version": "1.0.0",
	"description": "Post the scraping results on slack with cron jobs.",
	"scripts": {
		"start": "vercel dev --listen 5005",
		"dev": "pnpm start",
		"lint": "eslint .",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nonsugarless/scraping-notification.git"
	},
	"author": "nonsugarless",
	"dependencies": {
		"@vercel/fetch": "^6.2.0",
		"jsdom": "^21.1.0"
	},
	"devDependencies": {
		"@shopify/eslint-plugin": "^42.0.3",
		"@shopify/prettier-config": "^1.1.2",
		"@shopify/typescript-configs": "^5.1.0",
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.14.0",
		"@vercel/node": "^2.9.7",
		"eslint": "^8.34.0",
		"prettier": "^2.8.4",
		"typescript": "^4.9.5",
		"vercel": "^28.16.5"
	},
	"packageManager": "pnpm@8.2.0",
	"pnpm": {
		"overrides": {
			"typescript": "^4.9.5"
		},
		"peerDependencyRules": {
			"ignoreMissing": [
				"node-fetch",
				"@types/node-fetch",
				"@babel/core"
			]
		}
	}
}
